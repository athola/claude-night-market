  "agents": [
    "./agents/git-workspace-agent.md",
    "./agents/commit-agent.md",
    "./agents/pr-agent.md",
    "./agents/workflow-recreate-agent.md",
    "./agents/workflow-improvement-analysis-agent.md",
    "./agents/workflow-improvement-planner-agent.md",
    "./agents/workflow-improvement-implementer-agent.md",
    "./agents/workflow-improvement-validator-agent.md",
    "./agents/dependency-updater.md"
