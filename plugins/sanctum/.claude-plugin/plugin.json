  "skills": [
    "./skills/commit-messages",
    "./skills/do-issue",
    "./skills/doc-consolidation",
    "./skills/doc-updates",
    "./skills/file-analysis",
    "./skills/git-workspace-review",
    "./skills/pr-prep",
    "./skills/pr-review",
    "./skills/session-management",
    "./skills/shared",
    "./skills/test-updates",
