{
  "name": "sanctum",
  "version": "1.2.9",
  "description": "Git and workspace operations for active development workflows - commit messages, PR prep, documentation updates, and version management",
  "commands": [
    "./commands/git-catchup.md",
    "./commands/commit-msg.md",
    "./commands/do-issue.md",
