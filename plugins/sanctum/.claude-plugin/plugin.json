    "./commands/fix-pr.md",
    "./commands/merge-docs.md",
    "./commands/prepare-pr.md",
    "./commands/pr-review.md",
    "./commands/resolve-threads.md",
    "./commands/update-docs.md",
    "./commands/update-plugins.md",
    "./commands/update-readme.md",
    "./commands/update-tests.md",
    "./commands/update-version.md",
    "./commands/update-dependencies.md",
    "./commands/update-tutorial.md",
    "./commands/create-tag.md"
